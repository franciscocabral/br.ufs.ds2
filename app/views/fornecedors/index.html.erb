<p id="notice"><%= notice %></p>

<div class="portlet light">
  <div class="portlet-title">
    <div class="caption font-blue-sunglo">
      <span class="caption-subject bold uppercase">Lista de Fornecedores</span>
    </div>
  </div>
  <div class="portlet-body">
    <%= render partial: 'table', locals: {fornecedors: @fornecedors } %>

    <br>

    <%= link_to new_fornecedor_path, class: "btn green" do %>
      <i class="fa fa-plus-circle"></i>
      Novo Fornecedor
    <% end %>

    <%= link_to fornecedors_search_path, class: "btn green" do %>
      <i class="fa fa-search"></i>
      Consulta de fornecedor
    <% end %>
    
  </div>
</div>
